{
  "version": 2,
  "rewrites": [
    {
      "source": "/((?!api/)(?!css/)(?!js/)(?!assets/).*)",
      "destination": "/index.html"
    }
  ],
  "headers": [
    {
      "source": "/(.*)",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "public, max-age=0, must-revalidate"
        }
      ]
    }
  ]
}
